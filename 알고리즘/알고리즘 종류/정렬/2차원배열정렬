//JS
let myArr = [
  [1, 4],
  [3, 5],
  [0, 6],
  [5, 7],
  [3, 8],
  [5, 9],
  [6, 10],
  [8, 11],
  [8, 12],
  [2, 13],
  [12, 14],
];

console.log(myArr);
// arr[1]이 같으면 arr[0] 기준으로 오름차순 정렬
myArr.sort((a, b) => {
  if (a[1] === b[1]) {
    return a[0] - b[0];
  } else {
    return a[1] - b[1];
  }
});
console.log(myArr);

//파이썬
//1번 기준으로 오름차순 같으면 0번으로 오름차순
//k=sorted(k, key=lambda x: (x[1], x[0]))
